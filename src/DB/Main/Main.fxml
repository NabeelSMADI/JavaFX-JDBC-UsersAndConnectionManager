<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DB.Main.Main">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu fx:id="file" mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="ConnectBt" mnemonicParsing="false" onAction="#Connect" text="Connect" />
                  <MenuItem fx:id="UsersBt" mnemonicParsing="false" onAction="#openUsers" text="Users" />
            <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="logOutBt" mnemonicParsing="false" onAction="#logOut" text="logOut" />
            <MenuItem fx:id="closeBt" mnemonicParsing="false" onAction="#Exit" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="main" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
  </children>
</VBox>
